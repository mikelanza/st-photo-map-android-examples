<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.streetography.stphotomap.examples">

    <application android:allowBackup="true"
                 android:label="@string/app_name"
                 android:icon="@mipmap/ic_launcher"
                 android:roundIcon="@mipmap/ic_launcher_round"
                 android:supportsRtl="true"
                 android:theme="@style/AppTheme">

        <activity android:name=".scenes.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".scenes.photo_map_without_filters.PhotoMapWithoutFiltersActivity"
                    android:label="@string/photo_map_without_filters_activity_title"/>

        <meta-data
                android:name="com.google.android.maps.v2.API_KEY"
                android:value="${google_map_key}" />
    </application>
</manifest>
